<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="StaticKeysSection" type="TestprogLogManager.StaticKeysSection, TestprogLogManager"  />
  </configSections>

  <StaticKeysSection 
      MainFormCaption="TestprogLogManager Application"
      MinAppThreads="1"
      MaxAppThreads="25"
      RequireLogon="False"
      ValidBooleanValues ="true,false,yes,no,y,n,True,False,Yes,No,Y,N,TRUE,FALSE,YES,NO"
    />


  <appSettings>
    <add key="SaveErrorMessagesToErrorLog" value="true"/>
    <add key="ApplicationLogFileName" value="App.log"/>
    <add key="TextLogFileLocation" value="C:\Temp\TestAppFolders\Logs\Testlog.txt"/>
    <add key="XmlLogRetryQueueLocation" value="C:\Temp\TestAppFolders\Logs\Testlog_RetryQueue.xml"/>

  </appSettings>

  <startup>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
</startup>
</configuration>
